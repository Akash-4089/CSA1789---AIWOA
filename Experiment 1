a=int(input("Enter first number: "))
b=int(input("Enter second number: "))
c=int(input("Enter your choice: 1.Add 2.Sub 3.Mul 4.Div "))
if (c==1):
    print("Sum is ", a+b)
elif(c==2):
    print("Diff is ", a-b)
elif(c==3):
    print("Product is ", a*b)
elif(c==4 and b!=0):
    print("Quotient is ",a/b)
elif(c==4 and b==0):
    print("Div by 0 is not possible")
